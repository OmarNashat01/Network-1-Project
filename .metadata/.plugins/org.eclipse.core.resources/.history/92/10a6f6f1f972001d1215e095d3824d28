#include "Node0.h"

Define_Module(Node0);

//increment seq_nr circularly
bool between(int a, int b, int c)
{
    if (((a<=b) && (b<c)) || ((c<a) && (a<=b)) || ((b<c) && (c<a)))
        return true;
    else
        return false;
}

